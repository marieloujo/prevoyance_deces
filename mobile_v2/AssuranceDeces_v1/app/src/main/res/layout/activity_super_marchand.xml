<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:showIn="activity.SuperMarchandMainActivity">



    <include layout="@layout/content_appbar" />



    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/app_bar"
        android:id="@+id/content_main"
        android:orientation="vertical"/>





    <com.fxn.BubbleTabBar
        android:id="@+id/bubbleTabBarSm"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="#fff"
        android:elevation="20dp"
        android:padding="7dp"
        app:bubbletab_menuResource="@menu/bubbletabber_menu_supermarchand"
        app:bubbletab_horizontal_padding="20dp"
        app:bubbletab_icon_size="20dp"
        app:bubbletab_title_size="13sp"
        app:bubbletab_vertical_padding="15dp"
        tools:ignore="UnusedAttribute" />



</RelativeLayout>