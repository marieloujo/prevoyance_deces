<resources xmlns:tools="http://schemas.android.com/tools">

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/white</item>
        <item name="colorPrimaryDark">@color/black_light</item>
        <item name="colorAccent">@color/black_background</item>
    </style>

    <!-- Application theme. -->
    <style name="PrincipalTheme" parent="Theme.AppCompat.Light.NoActionBar">
        <item name="colorPrimary">@color/black</item>
        <item name="colorPrimaryDark">@color/black_light</item>
        <item name="colorAccent">@color/black_background</item>
    </style>


    <style name="SeekBar" parent="Theme.AppCompat.Light">
        <item name="android:colorControlActivated" tools:targetApi="lollipop">@color/white</item>
        <item name="android:colorControlNormal" tools:targetApi="lollipop">@color/white</item>
    </style>



    <style name="MyProgressBar" parent="@style/Widget.AppCompat.ProgressBar.Horizontal">
    </style>


</resources>
