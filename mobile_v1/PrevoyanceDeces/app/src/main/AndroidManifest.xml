<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    package="bj.assurance.prevoyancedeces">

    <uses-sdk tools:overrideLibrary="com.shrikanthravi.customnavigationdrawer2,com.gauravk.bubblenavigation,com.fxn.bubbletabbar, me.ibrahimsn.lib, br.com.simplepass.loadingbutton, com.arthurivanets.bottomsheets,com.shreyaspatil.MaterialNavigationView, com.tsongkha.spinnerdatepicker, com.skydoves.expandablelayout" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.CAMERA" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="whateverString"
        android:icon="@mipmap/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme"
        android:usesCleartextTraffic="true"
        tools:ignore="GoogleAppIndexingWarning"
        tools:replace="android:appComponentFactory"
        tools:targetApi="p">
        <activity
            android:name=".activity.SplashScreen"
            android:theme="@style/SplashSceenTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.Connexion"
            android:theme="@style/SplashSceenTheme" />
        <activity
            android:name=".activity.Main2Activity"
            android:label="@string/app_name"
            android:theme="@style/SplashSceenTheme" />
        <activity
            android:name=".activity.MarchandMainActivity"
            android:theme="@style/SplashSceenTheme" />
        <activity
            android:name=".activity.Welcome"
            android:theme="@style/SplashSceenTheme" />
        <activity android:name=".activity.ChatFrame" />
        <activity android:name=".activity.SuperMarchandMainActivity" />
        <activity android:name=".activity.MainVisiteurActivity" />
        <activity android:name=".activity.Test"></activity>
    </application>

</manifest>